{
  "name": "stardriver",
  "version": "1.0.0",
  "config": {
    "ctxname": "PruebaTecnicaContext",
    "ctx": "--output-dir Migrations --context PruebaTecnicaContext"
  },
  "scripts": {
    "migration": "npm run m-context && npm run update-db" ,
    "m-context": "dotnet-ef migrations add InitialCreate -s ../api/ %npm_package_config_ctx%",
    "update-db": "dotnet-ef database update InitialCreate -s ../api/ --context %npm_package_config_ctxname%"
  }
}